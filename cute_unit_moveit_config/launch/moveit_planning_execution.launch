<launch>
 # The planning and execution components of MoveIt! configured to 
 # publish the current configuration of the robot (simulated or real)
 # and the current state of the world as seen by the planner
    
 <param name="move_group/trajectory_execution/execution_duration_monitoring" value="false" />
 
 <include file="$(find cute_unit_moveit_config)/launch/move_group.launch">
  <arg name="publish_monitored_planning_scene" value="true" />
 </include>
 # The visualization component of MoveIt!
 <arg name="display" default="true"/>
 
 <include file="$(find cute_unit_moveit_config)/launch/moveit_rviz.launch" if="$(arg display)">
   <arg name="config" value="true"/>
 </include>
 
 <node name="cute_basic_api" pkg="cute_bringup" type="cute_basic_api.py" output="screen"/>
</launch>
